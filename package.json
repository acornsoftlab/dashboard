{
    "name": "acornsoft-dashboard",
    "version": "0.1.1",
    "description": "acornsoft kubernetes dashboard",
    "repository": {
        "type": "git",
        "url": "https://github.com/acornsoftlab/dashboard.git"
    },
    "license": "UNLICENSED",
    "scripts": {
        "start": "concurrently \"npm run start:dashboard\" \"npm run start:backend\" \"npm run start:frontend\"",
        "start:frontend": "nuxt",
        "start:backend": "cd src/app/backend; gulp serve",
        "start:dashboard": "cd dashboard; npm run start:backend;",
        "start:graph": "cd src/app/graph; npm run start",
        "start:graph:backend": "concurrently \"npm run start:backend\" \"npm run start:graph\"",
        "build:frontend": "nuxt build",
        "build:graph": "cd src/app/graph; npm run build; cd ..;cd ..;cd ..;cp src/app/graph/dist/*.js src/app/frontend/static/acorn-graph/",
        "docker:build:frontend": "./scripts/docker-build.sh frontend build $npm_package_config_docker_image_tag",
        "docker:build:backend": "./scripts/docker-build.sh backend build $npm_package_config_docker_image_tag",
        "docker:build:dashboard": "./scripts/docker-build.sh dashboard build $npm_package_config_docker_image_tag",
        "docker:push:frontend": "./scripts/docker-build.sh frontend push $npm_package_config_docker_image_tag",
        "docker:push:backend": "./scripts/docker-build.sh backend push $npm_package_config_docker_image_tag",
        "docker:push:dashboard": "./scripts/docker-build.sh dashboard push $npm_package_config_docker_image_tag",
        "docker:build:push:frontend": "./scripts/docker-build.sh frontend build:push $npm_package_config_docker_image_tag",
        "docker:build:push:backend": "./scripts/docker-build.sh backend build:push $npm_package_config_docker_image_tag",
        "docker:build:push:dashboard": "./scripts/docker-build.sh dashboard build:push $npm_package_config_docker_image_tag",
        "docker:build": "concurrently \"npm run docker:build:frontend\" \"npm run docker:build:backend\" \"npm run docker:build:dashboard\"",
        "docker:build:push": "concurrently \"npm run docker:build:push:frontend\" \"npm run docker:build:push:backend\" \"npm run docker:build:push:dashboard\"",
        "run": "nuxt start"
    },
    "dependencies": {
        "@nuxtjs/axios": "^5.12.2",
        "bootstrap-vue": "^2.0.0",
        "nuxt": "^2.14.11"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "1.7.0",
        "babel-register": "^6.26.0",
        "gulp": "4.0.2",
        "q": "^1.5.1",
        "@nuxt/typescript-build": "^0.6.0",
        "@fortawesome/fontawesome-free": "5.13.0",
        "ace-builds": "^1.4.11",
        "chart.js": "^2.9.3",
        "vue-chartjs": "^3.5.0",
        "expose-loader": "^0.7.5",
        "jquery": "3.4.1",
        "concurrently": "5.1.0"
    },
    "engines": {
        "node": ">=10.13.0",
        "npm": ">=6.0.0"
    }
}