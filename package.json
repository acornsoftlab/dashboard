{
    "name": "acornsoft-kore3",
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/acornsoftlab/kore3.git"
    },
    "license": "UNLICENSED",
    "scripts": {
        "dev:graph": "concurrently \"npm run start:backend\" \"npm run start:graph\"",
        "dev:ui": "concurrently  \"npm run start:dashboard\"  \"npm run start:backend\" \"npm run start:frontend\"",
        "start": "concurrently \"npm run start:dashboard\" \"npm run start:backend\" \"npm run start:graph\" \"npm run start:frontend\"",
        "start:frontend": "nuxt",
        "start:backend": "cd src/app/backend; gulp serve",
        "start:dashboard": "cd dashboard; npm run start:backend;",
        "start:graph": "webpack-dev-server --config src/app/graph/webpack.js --colors --mode=development",
        "build:graph:dev": "webpack --colors --config src/app/graph/webpack.js --mode=development",
        "build:graph": "webpack --colors --config src/app/graph/webpack.js --mode=production",
        "build:frontend": "nuxt build",
        "run": "nuxt start"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "5.13.0",
        "@types/chart.js": "^2.9.19",
        "ace-builds": "^1.4.11",
        "bootstrap": "4.3.1",
        "bootstrap-vue": "^2.0.0",
        "chart.js": "^2.9.3",
        "ionicons": "^5.0.1",
        "jquery": "3.4.1",
        "jquery-ui": "1.12.1",
        "nuxt": "^2.12.2",
        "vue-chartjs": "^3.5.0",
        "js-yaml": "^3.14.0",
        "@nuxtjs/axios": "^5.12.2",
        "@kubernetes/client-node": "^0.12.0",
        "d3": "^5.9.2",
        "d3-array": "^2.3.1"
    },
    "devDependencies": {
        "@nuxt/typescript-build": "^0.6.0",
        "babel-cli": "6.26.0",
        "babel-preset-env": "1.7.0",
        "babel-register": "6.26.0",
        "through2": "3.0.1",
        "expose-loader": "^0.7.5",
        "concurrently": "5.1.0",
        "gulp": "4.0.2",
        "webpack-dev-server": "^3.11.0",
        "webpack-cli": "^3.3.12",
        "html-webpack-plugin": "^4.4.1",
        "@types/d3": "^5.7.2",
        "@types/d3-hierarchy": "^1.1.6",
        "@types/d3-collection": "^1.0.8",
        "@types/d3-array": "^2.0.0",
        "style-loader": "^1.2.1"
    },
    "engines": {
        "node": ">=10.13.0",
        "npm": ">=6.0.0"
    }
}